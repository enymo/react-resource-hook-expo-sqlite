{
  "name": "@enymo/react-resource-hook-expo-sqlite",
  "version": "0.0.0",
  "description": "Expo SQLite adapter for the react resource hook",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "entry": [
      "src/js/index.tsx"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enymo/react-resource-hook-expo-sqlite.git"
  },
  "author": "enymo GmbH",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.1.8",
    "postcss": "^8.5.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@enymo/react-resource-hook": "^6.0.0-beta.0",
    "expo-sqlite": "^15.2.14",
    "react": ">=17.0.0"
  }
}
